{
  "name": "fil12/images-loader-from-url",
  "description": "This`s a images loadef from url.",
  "type": "library",
  "homepage": "https://github.com/fil12/images-loader-from-url",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Filimonov",
      "email": "filimonov12@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix --allow-risky=yes"
  }
}
